{
    "functions": {
        "api/*.py": {
          "includeFiles": ["api/templates/**"]
        }
    },
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ]
}
